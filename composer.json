{
    "name": "smartbox/integration-framework-bundle",
    "description": "Smartbox Framework Bundle",
    "type": "symfony-bundle",
    "license": "MIT",
    "autoload": {
      "psr-4": {
        "Smartbox\\Integration\\FrameworkBundle\\": ""
      }
    },
    "require": {
        "php": ">=7.0",
        "ext-pcntl": "*",
        "guzzlehttp/guzzle": "^6.0",
        "smartbox/besimple-soap": "^1.0.0",
        "smartbox/core-bundle": "~1.4",
        "symfony/framework-bundle": "^2.8|^3.0|^4.0"
    },
    "require-dev": {
        "doctrine/doctrine-bundle": "^1.6",
        "enqueue/amqp-ext": "^0.9.2",
        "incenteev/composer-parameter-handler": "^2.1",
        "jms/serializer-bundle": "*",
        "mongodb/mongodb": "~1.0",
        "stomp-php/stomp-php": "4.*",
        "symfony/monolog-bundle": "^2.4",
        "symfony/phpunit-bridge": "*"
    },
    "scripts": {
        "post-install-cmd": [
            "Incenteev\\ParameterHandler\\ScriptHandler::buildParameters"
        ],
        "post-update-cmd": [
            "Incenteev\\ParameterHandler\\ScriptHandler::buildParameters"
        ]
    },
    "config": {
        "bin-dir": "bin"
    },
    "minimum-stability": "dev",
    "prefer-stable" : true,
    "extra": {
        "incenteev-parameters": {
            "file": "Tests/App/config/parameters.yml"
        }
    },
    "suggest": {
        "enqueue/amqp-ext": "To use the AMQP driver."
    }
}
