{
    "name": "smartbox/integration-framework-bundle",
    "description": "Smartbox Framework Bundle",
    "type": "symfony-bundle",
    "license": "MIT",
    "autoload": {
      "psr-4": {
        "Smartbox\\Integration\\FrameworkBundle\\": ""
      }
    },
    "require": {
        "php": ">=5.6.0",
        "ext-pcntl": "*",
        "guzzlehttp/guzzle": "^6.0",
        "symfony/symfony": "^2.8|^3.0|^4.0",
        "doctrine/annotations": "^1.2.0",
        "symfony/monolog-bundle": "^2.4.0",
        "sensio/distribution-bundle": "^4.0.0",
        "sensio/framework-extra-bundle": "^3.0.0",
        "incenteev/composer-parameter-handler": "~2.0",
        "ocramius/proxy-manager": "~1.0",
        "stomp-php/stomp-php": "4.*",
        "smartbox/besimple-soap": "^1.0.0",
        "smartbox/core-bundle": "~1.4"
    },
    "require-dev": {
        "symfony/phpunit-bridge": "*",
        "doctrine/doctrine-bundle": "^1.6",
        "sensio/generator-bundle": "~2.3",
        "mongodb/mongodb": "~1.0",
        "jms/serializer-bundle": "*",
        "smartbox/camel-config-bundle": "dev-tidy/move-tests as v1.0.0",
        "ext-mongodb": "*"
    },
    "scripts": {
        "post-install-cmd": [
            "Incenteev\\ParameterHandler\\ScriptHandler::buildParameters"
        ],
        "post-update-cmd": [
            "Incenteev\\ParameterHandler\\ScriptHandler::buildParameters"
        ]
    },
    "config": {
        "bin-dir": "bin"
    },
    "minimum-stability": "dev",
    "prefer-stable" : true,
    "extra": {
        "incenteev-parameters": {
            "file": "Tests/App/config/parameters.yml"
        }
    }
}
